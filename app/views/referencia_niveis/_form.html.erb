<%= simple_form_for @referencia_nivel, :html => { :class => 'form-horizontal' } do |f| %>
<%= f.input :nome %>
<%= f.input :codigo %>
<%= f.input :vencimento_id %>
<%= f.input :entidade_id %>
<%= f.input :jornada %>

<%= f.button :submit, :class => 'btn-primary' %>
<%= link_to "Cancelar",
referencia_niveis_path, :class => 'btn btn-default' %>
<% end %>
