<%= simple_form_for @orgao,:url=>@url, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :codigo %>
  <%= f.input :nome %>
  <%= f.input :sigla %>
  <%= f.input :responsavel_id %>
  <%= f.input :telefone %>
  <%= f.input :endereco %>
  <%= f.input :numero %>
  <%= f.input :complemento %>
  <%= f.input :grupo %>
  <%= f.input :entidade_id %>
  <%= f.input :esfera_id %>
  <%= f.input :poder_id %>
  <%= f.input :tipo_administracao_id %>
  <%= f.button :submit, :class => 'btn-primary' %>
  <%= link_to "Cancelar",
orgaos_path, :class => 'btn btn-default' %>
<% end %>