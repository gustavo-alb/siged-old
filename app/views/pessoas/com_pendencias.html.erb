<div class="col-sm-12 col-md-12, well">
  <% @funcionarios.sem_nivel_e_nomeacao.group_by{|f|f.pessoa.nome}.each do |pessoa, funcionarios|%>
  <table class="table table-striped table-hover table-bordered table-condensed">
    <thead>
      <tr data-toggle='collapse' data-target='#minhas_atividades-<%= pessoa.object_id %>' class='accordion-toggle' >
        <th><p align='center'><i class="fa fa-eye fa-fw"></i>   <%= pessoa %></p></th>
      </tr>
    </thead>
    <tbody colspan='3' class='hiddenRow' class='accordian-body collapse' id='minhas_atividades-<%= pessoa.object_id %>'>
      <tr >
        <th>s</th>
        <th>s</th>
        <th>s</th>
      </tr>
      <% for funcionario in funcionarios %>
      <tr>
        <td><%= funcionario.matricula if funcionario.matricula %></td>
        <td><%= funcionario.pessoa.nome %></td>
        <td><%= funcionario.data_nomeacao if funcionario.data_nomeacao %></td>
      </tr>
      <%end%>
    </tbody>
  </table>





  <%end%>
