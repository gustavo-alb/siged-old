  <%= javascript_include_tag "mascara.js" %>
  <fieldset style="width:24%; float:left; margin-right: 1%;">
    <%= f.label :escola %><br />
    <%= f.autocomplete_field :escola, autocomplete_escola_nome_anos_letivos_path,:style=>"width:88%;"%>

  </fieldset>

  <fieldset style="width:22%; float:left; margin-right: 1%;">
    <%= f.label :disciplina %><br />
    <%= f.autocomplete_field :disciplina, autocomplete_disciplina_nome_anos_letivos_path,:style=>"width:88%;",'data-id-element' => '#order_customer_id'%>

  </fieldset>
  <fieldset style="width:16%; float:left; margin-right: 1%;">
    <%= f.label :status %><br />
    <%= f.select :status,[["CARÊNCIA ATUALIZADA 2015"],["CARÊNCIA PREENCHIDA"],["CARÊNCIA NÃO INFORMADA"]],:style=>"width:89%;" %>

  </fieldset>
  <fieldset style="width:5%; float:left; margin-right: 1%;">
    <%= f.label :carga_horaria %><br />
    <%= f.select :carga_horaria,[["20H",20],["40H",40]],:style=>"width:89%;" %>
  </fieldset>
  <fieldset style="width:5%; float:left; margin-right: 1%;">
    <%= f.label :data %><br />
    <%= f.text_field :data,:start_year=>1970,:order=>[:day,:month,:year],:style=>"width:40%;",:class=>"date",:value=>"#{f.object.data.strftime("%d/%m/%Y") unless f.object.new_record?}"%>
  </fieldset>