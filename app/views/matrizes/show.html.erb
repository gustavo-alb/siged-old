<%content_for :links do%>
		<%=link_to "Matrizes",matrizes_path%><div class="breadcrumb_divider"></div><a class="current">Detalhes</a>
<%end%>
<article class="module width_full">
<header><h3>Detalhes da matriz <%=@matriz.codigo%> <%= link_to 'Configurar matriz', matriz_configurar_curriculos_path(@matriz) %></h3></header>
<div class="module_content">

<p>
  <b>Codigo:</b>
  <%= @matriz.codigo %>
</p>

<p>
  <b>Nivel:</b>
  <%= detalhes(@matriz.nivel) %>
</p>

<p>
  <b>Módulo/Aula:</b>
  <%= @matriz.modulo_aula %>
</p>

<p>
  <b>Dias letivos anuais:</b>
  <%= @matriz.dias_letivos_anuais %>
</p>

<p>
  <b>Semanas letivas:</b>
  <%= @matriz.semanas_letivas %>
</p>

<p>
  <b>Carga horária anual:</b>
  <%= @matriz.carga_horaria_anual %>
</p>


</div>
<footer>
				<div class="submit_link">
				  <%= link_to 'Editar', edit_matriz_path(@matriz),:class=>"alt_btn",:type=>"submit"%> ou
				  <%= link_to 'Voltar', matrizes_path,:type=>"submit" %>
				</div>
			</footer>
</article>

