<%= simple_form_for @matriz, :html => { :class => 'form-horizontal' } do |f| %>
<%= f.input :codigo %>
<%= f.input :modalidade %>
<%= f.input :dias_letivos_anuais %>
<%= f.input :created_at %>
<%= f.input :updated_at %>
<%= f.input :entidade_id %>
<%= f.input :dias_letivos_semanais %>
<%= f.input :semanas_letivas %>
<%= f.input :modulo_aula %>
<%= f.input :carga_horaria_anual %>
<%= f.input :nivel_id %>
<%= f.input :nome %>

<%= f.button :submit, :class => 'btn-primary' %>
<%= link_to "Cancelar",
matrizes_path, :class => 'btn btn-default' %>
<% end %>
