  <%= javascript_include_tag "mascara.js" %>
  <fieldset style="width:22%; float:left; margin-right: 1%;">
    <%= f.label :escola_id %><br />
    <%= f.autocomplete_field :escola_id, autocomplete_escola_nome_da_escola_carencias_path(@ano_letivo),:style=>"width:88%;"%>

  </fieldset>

  <fieldset style="width:22%; float:left; margin-right: 1%;">
    <%= f.label :disciplina_id %><br />
    <%= f.autocomplete_field :disciplina_id, autocomplete_disciplina_nome_carencias_path(@ano_letivo),:style=>"width:88%;"%>

  </fieldset>
  <fieldset style="width:22%; float:left; margin-right: 1%;">
    <%= f.label :data %><br />
    <%= f.text_field :data,:start_year=>1970,:order=>[:day,:month,:year],:style=>"width:40%;",:class=>"date"%>

  </fieldset>
  <fieldset style="width:22%; float:left; margin-right: 1%;">
    <%= f.label :status %><br />
    <%= f.select :status,[["CARÊNCIA ATUALIZADA 2015"],["CARÊNCIA PREENCHIDA"],["CARÊNCIA NÃO INFORMADA"]],:style=>"width:89%;" %>
  </fieldset>