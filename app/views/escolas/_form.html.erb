<%= simple_form_for @escola, :html => { :class => 'form-horizontal' } do |f| %>
<%= f.input :codigo %>
<%= f.input :tipologia %>
<%= f.input :rede %>
<%= f.input :zona %>
<%= f.input :endereco %>
<%= f.input :telefone %>
<%= f.input :fax %>
<%= f.input :email %>
<%= f.input :celular %>
<%= f.input :numero %>
<%= f.input :complemento %>
<%= f.input :bairro %>
<%= f.input :gt_id %>
<%= f.input :diretor %>
<%= f.input :orgao_id %>
<%= f.input :lotacao_id %>
<%= f.input :cep %>
<%= f.input :cnpj %>
<%= f.input :tipo_destino_id %>
<%= f.input :municipio_id %>
<%= f.input :esfera_id %>
<%= f.input :entidade_id %>
<%= f.input :slug %>
<%= f.input :ano_letivo_id %>
<%= f.input :nome %>
<%= f.input :distrito %>

<%= f.button :submit, :class => 'btn-primary' %>
<%= link_to "Cancelar",
escolas_path, :class => 'btn btn-default' %>
<% end %>
