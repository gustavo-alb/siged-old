<article class="module width_full">
<header><h3>Detalhes de </h3></header>
<div class="module_content">


<p>
  <b>Ponto:</b>
  <%= @configuracao_ponto.ponto_id %>
</p>

<p>
  <b>Hora entrada manha:</b>
  <%= @configuracao_ponto.hora_entrada_manha %>
</p>

<p>
  <b>Tipo entrada manha:</b>
  <%= @configuracao_ponto.tipo_entrada_manha %>
</p>

<p>
  <b>Hora saida manha:</b>
  <%= @configuracao_ponto.hora_saida_manha %>
</p>

<p>
  <b>Tipo saida manha:</b>
  <%= @configuracao_ponto.tipo_saida_manha %>
</p>

<p>
  <b>Hora entrada tarde:</b>
  <%= @configuracao_ponto.hora_entrada_tarde %>
